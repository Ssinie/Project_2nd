<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="main">
	
	<select id="wishCheck" parameterType="productDTO" resultType="int">
		select clicked from wishlist where id=#{id}, num=#{num}
	</select>
	
	<insert id="wishInsert" parameterType="wishlistDTO">
		insert into wishlist(id, num) values(#{id}, #{num})
	</insert>
	
</mapper>