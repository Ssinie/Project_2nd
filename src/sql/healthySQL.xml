<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 
<mapper namespace="healthy" >
	<insert id="insertBoard" parameterType="HealthyDTO"> 
		insert into t_healthy_board(title, content, writer, regdate) values(#{title}, #{content}, #{writer}, sysdate)
	</insert>
	
	<select id="selectBoard" resultType="String">
		select * from t_healthy_board
	</select>
	
</mapper>